                  <TextField fx:id="commandLine" layoutX="106.0" layoutY="-1.0" prefHeight="72.0" prefWidth="640.0" promptText="%commandLine.promptText" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
